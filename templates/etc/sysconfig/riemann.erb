# Optionally add classes to the classpath for additional functionality
# EXTRA_CLASSPATH=

# Optional JAVA_OPTS
# EXTRA_JAVA_OPTS=
<%- unless @extra_java_opts.empty? -%>
EXTRA_JAVA_OPTS=<%= @extra_java_opts.join(' ') -%>
<%- end -%>

# Alternative path to config directory
# RIEMANN_PATH_CONF=

# Alternative path to config file
# RIEMANN_CONFIG=

# Additional options to pass to riemann on startup
# RIEMANN_OPTS=
